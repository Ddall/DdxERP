{# OrehaERP - Allan - Oreha:ErpBundle:Dossier:edit.html.twig #}
{% extends "::orehaTemplate.html.twig" %}
{% block title %}{{ parent()}} - Modifier {{ dossier.intitule }}{% endblock %}
{% block body%}
<div class="container-fluid">
    <div class="row-fluid">
        <div  class="span10">
            <fieldset><legend>Editer dossier - {{ dossier.intitule }}</legend>
                <div class="well">
                    <form method="post" {{ form_enctype(formDossier) }}>
                        {{ form_widget(formDossier) }}
                        <input type="submit" class="btn btn-primary" />
                        <a class="btn" href="{{ path('oreha_erp_dossier_voir', { 'id': dossier.id}) }}">Annuler</a>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div>
{% endblock body%}