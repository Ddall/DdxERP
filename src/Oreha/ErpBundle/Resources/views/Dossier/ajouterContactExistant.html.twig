{# OrehaERP - Allan - Oreha:ErpBundle:Dossier:ajouterContactExistant.html.twig #}
{% extends "::orehaTemplate.html.twig" %}
{% block title %}{{ parent()}} - Ajouter un contact{% endblock %}
{% block body%}
<div class="container-fluid">
    <div class="row-fluid">
        <div  class="span10">
            <fieldset><legend>Lier un contact existant au dossier {{ dossier.intitule|capitalize}}</legend>
                <div class="well">
                    <form method="post" {{ form_enctype(formSelect) }}>
                        {{ form_widget(formSelect) }}
                        <input type="submit" class="btn btn-primary" />
                        <a class="btn" href="{{ path('oreha_erp_contact') }}">Annuler</a>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div>
{% endblock body %}