{# OrehaERP - Allan - Oreha:ErpBundle:Bien:new.html.twig #}
{% extends "::orehaTemplate.html.twig" %}
{% block title %}{{ parent()}} - Ajouter un bien{% endblock %}
{% block body%}
<div class="container-fluid">
    <div class="row-fluid">
        <div  class="span10">
            <fieldset>
                <legend>
                    Dossier {{ dossier.intitule }} - Ajouter un bien
                    <a class="btn" href="{{ path('oreha_erp_dossier_voir', {'id': dossier.id}) }}"><span class="icon-arrow-left"></span> Retour</a>
                </legend>
                <div class="well">
                    <form method="post" {{ form_enctype(formBien) }}>
                        {{ form_widget(formBien) }}
                        <input type="submit" class="btn btn-primary" />
                        <a class="btn" href="{{ path('oreha_erp_dossier_voir', {'id': dossier.id}) }}">Annuler</a>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div>
{% endblock body %}